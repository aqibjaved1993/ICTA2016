#Script for latency of Routing algorithm for ICTA2016
set term postscript eps color blacktext "DE" 24
set out "ICTA2016_latency_DVOPD.eps"
set title "RA_Latency_DVOPD"
set xlabel "Packet Injection Rate (PIR)"
#set xrange [0:10]
set ylabel "Latency (cycles)"
#set key top left
set key bottom right
plot "XY_RA_out_DVOPD" using 1:4 title "XY_RA" lw 10 lc rgb "blue", \
"WEST_FIRST_RA_out_DVOPD" using 1:4 title "WEST_FIRST_RA" lw 10 lc rgb "red",\
"NORTH_LAST_RA_out_DVOPD" using 1:4 title "NORTH_LAST_RA" lw 10 lc rgb "black",\
"NEGATIVE_FIRST_RA_out_DVOPD" using 1:4 title "NEGATIVE_FIRST_RA" lw 10 lc rgb "green",\
"ODD_EVEN_RA_out_DVOPD" using 1:4 title "ODD_EVEN_RA" lw 10 lc rgb "gray"
#set output
quit
#lt rgb "#FF00FF"


